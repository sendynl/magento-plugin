{
    "name": "edifference/sendy",
    "description": "Sendy plug-in for Magento 2",
    "type": "magento2-module",
    "version": "1.0.9",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "authors": [
        {
            "name": "eDifference",
            "email": "info@edifference.nl",
            "homepage": "https://edifference.nl"
        }
    ],
    "require": {
        "php": ">=8.2",
        "sendynl/php-sdk": "^2.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Edifference\\Sendy\\": ""
        }
    },
    "require-dev": {
        "magento/magento-coding-standard": "^36.0",
        "magento/framework": "^103.0"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
